{
    "prettifySymbolsMode.substitutions": [
        {
            "language": "rust",
            "substitutions": [
                {
                    "ugly": "fun",
                    "pretty": "λ",
                    "scope": "keyword.other.function-definition.fsharp"
                },
                {
                    "ugly": "->",
                    "pre": "[^->]",
                    "post": "[^->]",
                    "pretty": "⟶"
                },
                {
                    "ugly": ".+",
                    "scope": "variable.parameter.fsharp",
                    "pre": "^",
                    "post": "$",
                    "style": {
                        "border": "1pt solid green"
                    }
                }
            ]
        }
    ]
}